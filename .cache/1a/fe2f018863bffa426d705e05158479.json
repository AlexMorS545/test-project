d-holder{\n        width: 100%;\n    }\n    &__card-date {\n        width: 70px;\n    }\n    &__card-cvv {\n        width: 70px;\n        position: absolute;\n        right: 150px;\n        bottom: 20px;\n    }\n}\n.credit {\n    width: 590px;\n    position: absolute;\n    background-color: #c6edf8;\n    border: 1px solid #000;\n    border-radius: 20px;\n    padding: 20px 50px;\n    z-index: 2;\n    top: -20%;\n    left: 23.5%;\n    &__logo {\n        text-align: end;\n    }\n    &__image {\n        width: 100px;\n    }\n    &__input {\n        width: 100%;\n        margin: 40px 0 10px;\n        border: none;\n        outline: none;\n        box-sizing: border-box;\n        background-color: transparent;\n        font-size: 20px;\n        font-weight: 500;\n    }\n    &__holder-wrp {\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n        margin-top: 10px;\n        font-size: 12px;\n    }\n    &__name-wrp {\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n        margin-top:5px;\n        font-size: 25px;\n        font-weight: 600;\n    }\n    &__holder,\n    &__expires {\n        margin: 0;\n    }\n    &__name,\n    &__date {\n        margin: 0;\n        text-transform: uppercase;\n        border: none;\n        outline: none;\n        box-sizing: border-box;\n        background-color: transparent;\n        padding: 0;\n    }\n    &__date {\n        width: 90px;\n        text-align: end;\n    }\n}"},"lineCount":null}},"error":null,"hash":"3b5c4a2731b00f6d6198e9784310857e","cacheData":{"env":{}}}